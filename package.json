{
  "name": "ai-command-center",
  "version": "2.0.0",
  "description": "Unified AI dashboard - Memory Viewer, Vision, Chain Runner",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "build:electron": "vite build && electron-builder",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "node test-api.js",
    "type-check": "tsc --noEmit"
  },
  "author": "Myers",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@tiptap/extension-image": "^3.14.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "diff": "^5.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "googleapis": "^169.0.0",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "mime": "^4.1.0",
    "node-pty": "^1.1.0",
    "node-ssh": "^13.2.1",
    "openai": "^4.20.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^2.1.7",
    "react-virtualized-auto-sizer": "^2.0.0",
    "react-window": "^2.2.3",
    "sqlite-vss": "^0.1.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@electron/rebuild": "^4.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.2",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "png-to-ico": "^3.0.1",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.myers.ai-command-center",
    "productName": "AI Command Center",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "node_modules/node-pty/**/*",
      "node_modules/bindings/**/*"
    ],
    "win": {
      "target": "dir",
      "icon": "src/assets/icon.ico",
      "sign": false,
      "signAndEditExecutable": false
    },
    "extraMetadata": {
      "main": "electron/main.cjs"
    },
    "buildDependenciesFromSource": false,
    "npmRebuild": false
  }
}
